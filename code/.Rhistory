library(Seurat)
library(dplyr)
library(cowplot)
library(harmony)
library(SingleCellExperiment)
library(tidyverse)
library(Matrix)
library(scales)
library(cowplot)
library(RCurl)
d18.data <- Read10X(data.dir = "~/Desktop/singleCell_IEO_timepoints/e2_d13-36 integrration/d18/outs/filtered_feature_bc_matrix/")
d24.data <- Read10X(data.dir = "~/Desktop/singleCell_IEO_timepoints/e2_d13-36 integrration/d24/outs/filtered_feature_bc_matrix/")
d30.data <- Read10X(data.dir = "~/Desktop/singleCell_IEO_timepoints/e2_d13-36 integrration/d30/outs/filtered_feature_bc_matrix/")
d36.data <- Read10X(data.dir = "~/Desktop/singleCell_IEO_timepoints/e2_d13-36 integrration/d36/outs/filtered_feature_bc_matrix/")
IEO <- CreateSeuratObject(counts = cbind(d18.data, d24.data, d30.data, d36.data), project = "IEO", min.cells = 5)
IEO@meta.data$sample <- c(rep("day 18", ncol(d18.data)), rep("day 24", ncol(d24.data)), rep("day 30", ncol(d30.data)), rep("day 36", ncol(d36.data)))
# Add number of genes per UMI for each cell to metadata
IEO$log10GenesPerUMI <- log10(IEO$nFeature_RNA) / log10(IEO$nCount_RNA)
# Compute percent mito ratio
IEO$mitoRatio <- PercentageFeatureSet(object = IEO, pattern = "^MT-")
IEO$mitoRatio <- IEO@meta.data$mitoRatio / 100
# Create metadata dataframe
metadata <- IEO@meta.data
# Add cell IDs to metadata
metadata$cells <- rownames(metadata)
# Rename columns
metadata <- metadata %>%
dplyr::rename(seq_folder = orig.ident,
nUMI = nCount_RNA,
nGene = nFeature_RNA)
# Add metadata back to Seurat object
IEO@meta.data <- metadata
# Visualize the number of cell counts per sample
metadata %>%
ggplot(aes(x=sample, fill=sample)) +
geom_bar() +
theme_classic() +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1)) +
theme(plot.title = element_text(hjust=0.5, face="bold")) +
ggtitle("NCells")
# Visualize the number UMIs/transcripts per cell
metadata %>%
ggplot(aes(color=sample, x=nUMI, fill= sample)) +
geom_density(alpha = 0.2) +
scale_x_log10() +
theme_classic() +
ylab("Cell density") +
geom_vline(xintercept = 500)
# Visualize the distribution of genes detected per cell via histogram
metadata %>%
ggplot(aes(color=sample, x=nGene, fill= sample)) +
geom_density(alpha = 0.2) +
theme_classic() +
scale_x_log10() +
geom_vline(xintercept = 300)
# Visualize the distribution of genes detected per cell via boxplot
metadata %>%
ggplot(aes(x=sample, y=log10(nGene), fill=sample)) +
geom_boxplot() +
theme_classic() +
theme(axis.text.x = element_text(angle = 45, vjust = 1, hjust=1)) +
theme(plot.title = element_text(hjust=0.5, face="bold")) +
ggtitle("NCells vs NGenes")
# Visualize the correlation between genes detected and number of UMIs and determine whether strong presence of cells with low numbers of genes/UMIs
metadata %>%
ggplot(aes(x=nUMI, y=nGene, color=mitoRatio)) +
geom_point() +
scale_colour_gradient(low = "gray90", high = "black") +
stat_smooth(method=lm) +
scale_x_log10() +
scale_y_log10() +
theme_classic() +
geom_vline(xintercept = 500) +
geom_hline(yintercept = 250) +
facet_wrap(~sample)
# Visualize the distribution of mitochondrial gene expression detected per cell
metadata %>%
ggplot(aes(color=sample, x=mitoRatio, fill=sample)) +
geom_density(alpha = 0.2) +
scale_x_log10() +
theme_classic() +
geom_vline(xintercept = 0.2)
# Visualize the overall complexity of the gene expression by visualizing the genes detected per UMI
metadata %>%
ggplot(aes(x=log10GenesPerUMI, color = sample, fill=sample)) +
geom_density(alpha = 0.2) +
theme_classic() +
geom_vline(xintercept = 0.8)
# Filter out low quality reads using selected thresholds - these will change with experiment
filtered_IEO <- subset(x = IEO,
subset= (nUMI >= 500) &
(nGene >= 250) &
(log10GenesPerUMI > 0.80) &
(mitoRatio < 0.20))
pdf("figures/UncorrectedPCs.pdf")
p1 <- DimPlot(object = IEO, reduction = "pca", pt.size = .1, group.by = "sample")
filtered_IEO <- Seurat::NormalizeData(filtered_IEO, verbose = FALSE) %>%
FindVariableFeatures(selection.method = "vst", nfeatures = 2000) %>%
ScaleData(verbose = FALSE) %>%
RunPCA(pc.genes = IEO@var.genes, npcs = 30, verbose = FALSE)
